{
  "name": "game-off-2016",
  "version": "1.0.0",
  "description": "Game for Github Gameoff 2016",
  "main": "dist/index.html",
  "scripts": {
    "deploy-libs": "cp node_modules/pixi.js/bin/pixi.min.* dist/",
    "deploy-assets": "cp -r src/assets dist",
    "deploy-static": "cp src/index.html dist/",
    "deploy": "mkdir -p dist; npm run deploy-libs; npm run deploy-assets; npm run deploy-static",
    "start": "cd dist; live-server"
  },
  "repository": {
    "url": "git+https://github.com/ropilz/game-off-2016.git",
    "type": "git"
  },
  "keyworkds": [
    "ggo16",
    "game off"
  ],
  "author": "ropilz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ropilz/game-off-2016/issues"
  },
  "dependencies": {
    "@types/pixi.js": "^3.0.30",
    "live-server": "^1.1.0",
    "pixi.js": "^4.1.1",
    "typescript": "^2.0.7"
  }
}
